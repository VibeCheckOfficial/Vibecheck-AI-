{
  "name": "@vibecheck/mock-detector",
  "version": "1.0.0",
  "description": "Detect and eliminate mock, fake, and placeholder data from your codebase",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "vibecheck": "./dist/cli/index.js"
  },
  "scripts": {
