    "glob": "^10.3.10",
    "hono": "^4.0.0",
    "typescript": "^5.3.0",
    "yaml": "^2.3.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
